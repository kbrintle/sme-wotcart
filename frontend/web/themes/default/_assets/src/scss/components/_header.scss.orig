.tel-number-mobile {
  background: $darkerBlue;
  padding: 15px;
  border-radius: 4px 4px 0 0;
  position: fixed;
  z-index: 9999;
  bottom: 0;
  left: 15px;
  right: 15px;
  box-shadow: 0px 0px 8px 3px rgba(0, 0, 0, 0.1);

  i {
    vertical-align: middle;
    color: $white;
    margin: -5px 0 0;
  }

  .tel-number {
    color: $white;
    letter-spacing: 2px;
    font-size: $f-size-lg;
    text-transform: uppercase;

  }
}