.search-bar {
  padding: 10px 0;

  .search-box {

    background: #fff;
    padding: 0 8px;
    color: $smeDarkGreen;
    border-radius: 3px;

    ::placeholder {
      color: $lightGray;
      letter-spacing: 1.2px;
      font-weight: 300;
      font-size: 14px;
    }

    :-ms-input-placeholder {
      color: $lightGray;
      letter-spacing: 1.2px;
      font-weight: 300;
      font-size: 14px;
    }

    input {
      outline: none;
      position: relative;
      width: 90%;
      padding: 3px;
      vertical-align: top;
      border: none;
      color: $smeDarkGreen;

      &:hover {
        outline: none;
      }

      &:focus {
        outline: none;
      }

    }

    .search-icon {
      position: relative;
      width: 10%;
      padding: 3px;
      font-size: 16px;
      color: $smeDarkGreen;
    }
  }
}

